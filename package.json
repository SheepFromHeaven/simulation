{
  "name": "webpack-ts-boilerplate",
  "version": "0.1.0",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/sinon": "^2.2.1",
    "@types/sinon-chai": "^2.7.27",
    "babel-loader": "^7.0.0",
    "chai": "^3.5.0",
    "eslint": "^4.2.0",
    "expose-loader": "^0.7.3",
    "mocha": "^3.3.0",
    "nyc": "^10.3.2",
    "phaser": "^2.6.2",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0",
    "ts-loader": "^2.0.3",
    "ts-node": "^3.0.4",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "start": "node node_modules/.bin/webpack-dev-server",
    "build": "node node_modules/.bin/webpack --config webpack.production.config.js",
    "test": "node node_modules/.bin/mocha --opts mocha.opts",
    "coverage": "node node_modules/.bin/nyc node_modules/.bin/mocha --opts mocha.opts"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ],
    "reporter": [
      "html"
    ],
    "extension": [
      ".ts"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  },
  "dependencies": {
    "redux": "^3.7.1",
    "redux-devtools-extension": "^2.13.2"
  }
}
